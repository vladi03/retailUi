{
  "name": "retailui",
  "version": "0.1.0",
  "description": "Display Catalog items of a retail store",
  "main": "index.js",
  "scripts": {
    "build:local": "nwb build-react-app --copy-files --config ./nwb.dev.config.js",
    "build": "nwb build-react-app --copy-files --config ./nwb.prod.config.js",
    "test": "mocha  -r esm --recursive ./test",
    "deploy:docs": "gh-pages -d dist",
    "start": "nwb serve-react-app --copy-files NODE_ENV=development --config ./nwb.dev.config.js",
    "start-prod": "nwb serve-react-app --copy-files NODE_ENV=prod --config ./nwb.prod.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/vladi03/retailUi.git"
  },
  "author": "Vlad Martinez",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/vladi03/retailUi/issues"
  },
  "homepage": "https://github.com/vladi03/retailUi#readme",
  "devDependencies": {
    "@material-ui/core": "^4.12.4",
    "@material-ui/icons": "^4.11.3",
    "fancy-menu": "^0.12.1",
    "gh-pages": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "pic-ratio-fill": "^0.3.17"
  },
  "dependencies": {
    "esm": "^3.2.25"
  }
}
